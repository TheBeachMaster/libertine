function build_extract()
{
	local -r name="$1"
	local -r download_kind="$2"
	local -r version="$3"
	local -r archive="$4"
	local -r tarbomb="$5"
	local -r archiveFolder="$6"
	
	local -r decompressedDownloadPath="$(readlink --canonicalize "$LFS_BUILD_ROOT_PATH_RECIPE"/downloads/"$download_kind"/"$name"/"$version")"
		
	local -r extractParentFolder="$LFS_BUILD_ROOT_PATH_RECIPE"/extracts/"$download_kind"/"$name"/"$version"
	rm -rf "$extractParentFolder"
	
	case "$archive" in
		
		none)
			file_hardlinkOrCopy "$decompressedDownloadPath" "$extractParentFolder"
			
			LFS_DOWNLOADS_PACKAGE_EXTRACT_PATHS["$name"]="$extractParentFolder"
		;;
		
		tarball)
			local extractFolderPath
			if [ "$tarbomb" = "yes" ]; then
				extractFolderPath="$extractParentFolder"/"$archiveFolder"
			else
				extractFolderPath="$extractParentFolder"
			fi
			
			mkdir -m 0755 -p "$extractFolderPath"

			pushd "$extractParentFolder" 1>/dev/null 2>/dev/null

				tar --extract --numeric-owner --preserve-permissions --no-same-owner --file "$decompressedDownloadPath"

			popd 1>/dev/null 2>/dev/null
		;;
		
		zip)
			lfs_exitError "Zip is not supported at this time for extract ($name)"
		;;
		
		*)
			lfs_exitError "Unknown archive format '$archive' for $download_kind $name"
		;;
		
	esac
}

function build_extractPackage()
{
	build_extract "$name" "package"  "${LFS_DOWNLOADS_PACKAGE_VERSIONS["$name"]}" "${LFS_DOWNLOADS_PACKAGE_ARCHIVES["$name"]}" "$LFS_DOWNLOADS_PACKAGE_TARBOMBS{["$name"]}" "${LFS_DOWNLOADS_PACKAGE_ARCHIVE_FOLDERS["$name"]}"
}

function build_extractPackages()
{
	local name
	for name in "${LFS_DOWNLOADS_PACKAGE_NAMES[@]}"
	do
		build_extractPackage
	done
}

function build_patchPackage()
{
	local -r packageName="$1"
	local -r patchName="$2"
	
	local -r downloadPath="$(readlink --canonicalize "$LFS_BUILD_ROOT_PATH_RECIPE"/downloads/patch/"$patchName")"
	local -r decompressedDownloadPath="$downloadPath".decompressed
	
	# Won't work too well!
	pushd "${LFS_DOWNLOADS_PACKAGE_EXTRACT_PATHS["$packageName"]}" 1>/dev/null 2>/dev/null
		
		patch -N -p1 -i "$decompressedDownloadPath"
		
	popd 1>/dev/null 2>/dev/null
}

function build_patchPackageFromIndex()
{
	local -r packageName="$1"
	local -ir patchIndex=$2
	
	build_patchPackage "$packageName" "${LFS_DOWNLOADS_PATCH_NAMES[$patchIndex]}"
}

function build_pushPackage()
{
	local -r name="$1"
	pushd "${LFS_DOWNLOADS_PACKAGE_EXTRACT_PATHS["$name"]}" 1>/dev/null 2>/dev/null
}

function build_popPackage()
{
	popd 1>/dev/null 2>/dev/null
}

function build_push()
{
	pushd "$1" 1>/dev/null
}

function build_pop()
{
	popd 1>/dev/null 2>/dev/null
}

function build_extractedTarballSourcePath()
{
	local -r name="$1"
	echo -ne "${LFS_DOWNLOADS_PACKAGE_EXTRACT_PATHS["$name"]}"/"${LFS_DOWNLOADS_PACKAGE_ARCHIVE_FOLDERS["$name"]}"
}

function build_outOfTree()
{
	# Put a build folder parallel to the tar folder
	local -r nameOfPackage="$1"
	local -r buildIteration="$2"
	shift 2
	
	local -r buildFolder="build-$buildIteration"
	
	build_pushPackage "$nameOfPackage"
		
		mkdir -m 0755 -p "$buildFolder"
		
		build_push "$buildFolder"
		
			../configure "$@"
			make "${makeFlags[@]}"
			make install "${makeInstallFlags[@]}"
			make distclean
		
		build_pop
		
	build_popPackage
}

function build_inTree()
{
	local -r nameOfPackage="$1"
	shift 1
	
	build_pushPackage "$nameOfPackage"
	
		./configure "$@"
		make "${makeFlags[@]}"
		make install "${makeInstallFlags[@]}"
		make distclean
		
	build_popPackage
}







