addSourcePackageDownload 3.11.1 "$LFS_MIRROR_LINUX_v3"/"$LFS_SOURCE_PACKAGE_NAME"-3.11.1.tar.xz MD5:466c042aa58d4839396443bf4438fdc5

function installPackage_toolchain1_linux()
{
	make mrproper
	
	make headers_check || echo "Warning: non-fatal but ignored as exit code !=0"
	
	make INSTALL_HDR_PATH=dest headers_install
	
	mkdir -m 0755 -p /"$LFS_TOOLS"/include
	cp -rv dest/include/* /"$LFS_TOOLS"/include
}
