set -o errexit +o hashall;unset CDPATH;umask 022
registerDebianPackage bash
essentialProgramCheck bash awk sed
bash --version | awk 'NR==1 {print $(NF-1) }' | sed -e 's/-.*//g' -e 's/(\([0-9]*\))/\.\1/g' | verifyDependency bash 3.2
