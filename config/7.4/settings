include_locale en_US UTF-8 en_US.UTF-8
include_locale es_MX ISO-8859-1 es_MX
include_locale fa_IR UTF-8 fa_IR
include_locale fr_FR ISO-8859-1 fr_FR
include_locale fr_FR@euro ISO-8859-15 fr_FR@euro
include_locale fr_FR UTF-8 fr_FR.UTF-8
include_locale it_IT ISO-8859-1 it_IT
include_locale it_IT UTF-8 it_IT.UTF-8
include_locale ja_JP EUC-JP ja_JP
