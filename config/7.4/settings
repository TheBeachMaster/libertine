
declare -ir LFS_INCLUSIVE_SYSTEM_UID=100
declare -ir LFS_INCLUSIVE_SYSTEM_GID=100
declare -ir LFS_INCLUSIVE_USER_UID=1000
declare -ir LFS_INCLUSIVE_USER_GID=1000

