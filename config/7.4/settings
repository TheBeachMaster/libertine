include_locale ru_RU UTF-8 ru_RU.UTF-8
include_locale tr_TR UTF-8 tr_TR.UTF-8
include_locale zh_CN GB18030 zh_CN.GB18030

declare -r LFS_TIMEZONE_POSIXRULES=America/New_York
