set -o errexit +o hashall

version 3.10.10
{
	depends binutils
}

function installPackage_crosscompiler_linux()
{
	make mrproper
	
	make headers_check || echo "Warning: non-fatal but ignored as exit code !=0"
	
	make INSTALL_HDR_PATH=dest headers_install
	
	cp -rv dest/include/* "$LFS_DEPENDENCY_INSTALL_PATH"/usr/include
}
